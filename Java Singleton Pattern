import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
import java.lang.reflect.*;


class Singleton
{
   private static Singleton s=null;
   public String str;
   private Singleton()
   {
      //System.out.println("Empty Constructor");  
   }
    static  public Singleton getSingleInstance()
    {
        if(s==null)
        {
            s=new Singleton();
        }
        return s;
    }
    
}

