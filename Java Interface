class MyCalculator implements AdvancedArithmetic
{
    public int divisor_sum(int n)
    {
       int resultValue =0;
       
       for(int i=1;i<=n;i++)
       {
          if(n%i == 0)
          {
             resultValue=resultValue+i;
          }
       }
       return resultValue;
    }
    
}
