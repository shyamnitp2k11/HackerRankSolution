import java.util.*;import java.io.*;
class Solution
{    public static void main(String []argh){        Scanner in = new Scanner(System.in);        int t=in.nextInt();        for(int i=0;i<t;i++){             int suminter=0,sum=0;            int a = in.nextInt();            int b = in.nextInt();            int n = in.nextInt();          for(int j=0;j<n;j++)          {              suminter=0;            for(int x=0;x<=j;x++){              suminter+=Math.pow(2,x);            }              sum=b*suminter+a;              System.out.print(sum+" ");          }          System.out.println();        }        in.close();    }}
